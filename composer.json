{
    "name": "webjeyros/webarchive-parser",
    "type": "library",
    "description": "Laravel Web Archive parser for finding available domains and content harvesting",
    "keywords": ["wayback-machine", "web-archive", "seo", "domain-parser", "content-harvesting"],
    "license": "MIT",
    "authors": [
        {
            "name": "webjeyros",
            "email": "webjeyros@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/tinker": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
