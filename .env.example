APP_NAME="Web Archive Parser"
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=webarchive_parser
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@example.com"
MAIL_FROM_NAME="Web Archive Parser"

# Web Archive API (Wayback Machine)
WAYBACK_API_URL=https://archive.org/wayback/available

# WHOIS server
WHOIS_SERVER=whois.com
WHOIS_TIMEOUT=10

# seo-rank API
SEORANK_API_KEY=your_api_key_here
SEORANK_API_URL=https://api.seo-rank.com
SEORANK_RATE_LIMIT=1000
SEORANK_COST_PER_1K=0.04

# Yandex API for uniqueness check
YANDEX_API_KEY=your_yandex_api_key

# Rate limiting
WAYBACK_PARSER_PER_PAGE=100
WAYBACK_TIMEOUT=30
HTTP_CHECK_TIMEOUT=5
WHOIS_BATCH_SIZE=50
SEORANK_BATCH_SIZE=100
